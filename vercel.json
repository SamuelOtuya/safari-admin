{
    "functions": {
      "pages/api/upload.js": {
        "maxDuration": 30
      },
      "pages/api/delete.js": {
        "maxDuration": 10
      }
    },
    "headers": [
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/api/upload",
        "destination": "/api/upload"
      }
    ]
  }